module Main

main = with a = debug (enum A),
            b = debug (take_enum a),
            c = debug iffy_enum,
            d = debug matchy_enum,
            e = debug (curry_enum enum A),
            f = debug (do_enum (curry_enum enum A)),
            0

take_enum a: enum A = a
curry_enum a: enum A, b: enum A = b
do_enum a: enum A -> enum A = a enum A
iffy_enum =
    if true then
        enum A
    else
        enum B
matchy_enum =
    match 2 : Int | Float
    to Int => enum A
    to Float => enum B
